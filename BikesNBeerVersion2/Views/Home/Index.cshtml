
@model BikesNBeerVersion2.Services.Rootobject





@{
    ViewData["Title"] = "Address";
}
<h1>@ViewData["Adddress"]</h1>



<table class="table table-striped">
    <thead>
        <tr>
            <th>
                Address Details
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.results!=null)
        {
            @foreach (BikesNBeerVersion2.Services.Result result in Model.results)
            {
                <tr>
                    <td>
                        @result.formatted_address
                    </td>
                </tr>
            }
        }


    </tbody>
</table>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Index">
            <div class="form-group">
                <label class="control-label"></label>
                <input name="zipcode" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="ZipCode" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
